@{
    Layout = null;
}
@using real_estate_web_application.Models;

@{
    Kullanicilar k = (Kullanicilar)Session["Kullanici"];
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="Mosaddek">
    <meta name="keyword" content="FlatLab, Dashboard, Bootstrap, Admin, Template, Theme, Responsive, Fluid, Retina">
    <link rel="shortcut icon" href="/Content/Admin/img/favicon.png">

    <title>Emlak</title>

    <!-- Bootstrap core CSS -->
    <link href="/Content/Admin/css/bootstrap.min.css" rel="stylesheet">
    <link href="/Content/Admin/css/bootstrap-reset.css" rel="stylesheet">
    <!--external css-->
    <link href="/Content/Admin/assets/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link href="/Content/Admin/assets/jquery-easy-pie-chart/jquery.easy-pie-chart.css" rel="stylesheet" type="text/css" media="screen" />
    <link rel="stylesheet" href="/Content/Admin/css/owl.carousel.css" type="text/css">
    <!-- Custom styles for this template -->
    <link href="/Content/Admin/css/style.css" rel="stylesheet">
    <link href="/Content/Admin/css/style-responsive.css" rel="stylesheet" />

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 tooltipss and media queries -->
    <!--[if lt IE 9]>
      <script src="/Content/Admin/js/html5shiv.js"></script>
      <script src="/Content/Admin/js/respond.min.js"></script>
    <![endif]-->
    @RenderSection("head", false)


</head>

<body>

    <section id="container">
        <!--header start-->
        <header class="header white-bg">
            <div class="sidebar-toggle-box">
                <div data-original-title="Toggle Navigation" data-placement="right" class="icon-reorder tooltips"></div>
            </div>
            <!--logo start-->
            <a href="/Content/Admin/index.html" class="logo">DepWeb<span>Emlak</span></a>
            <!--logo end-->

            <div class="top-nav ">
                <!--search & user info start-->
                <ul class="nav pull-right top-menu">
                    <li>
                        <input type="text" class="form-control search" placeholder="Search">
                    </li>
                    <!-- user login dropdown start-->
                    <li class="dropdown">
                        <a data-toggle="dropdown" class="dropdown-toggle" href="/Content/Admin/#">
                            <img alt="" src="/Content/Admin/img/avatar1_small.jpg">
                            <span class="username"><b>@k.ad</b> <b>@k.soyad</b></span>
                            <b class="caret"></b>
                        </a>
                        <div class="log-arrow-up"></div>
                        <ul class="dropdown-menu extended logout">
                            <li><a href="@Url.Action("cikisYap")"><i class="icon-key"></i> Çıkış Yap</a></li>
                        </ul>
                    </li>
                    <!-- user login dropdown end -->
                </ul>
                <!--search & user info end-->
            </div>
        </header>
        <!--header end-->
        <!--sidebar start-->
        <aside>
            <div id="sidebar" class="nav-collapse ">
                <!-- sidebar menu start-->
                <div id="sidebar" class="nav-collapse ">
                    <!-- sidebar menu start-->
                    <ul class="sidebar-menu" id="nav-accordion">
                        <li>
                            <a class="active" href="/Admin/Index">
                                <i class="icon-dashboard"></i>
                                <span>Anasayfa</span>
                            </a>
                        </li>

                        <li class="sub-menu">
                            <a href="siteOzellikleri">
                                <i class="icon-laptop"></i>
                                <span>Site Bilgileri</span>
                            </a>
                        </li>

                        <li class="sub-menu">
                            <a href="/Content/Admin/javascript:;">
                                <i class="icon-book"></i>
                                <span>Kullanıcı Bilgileri</span>
                            </a>
                            <ul class="sub">
                                <li><a href="/Admin/kullaniciListele">Kullanıcı Listesi</a></li>
                            </ul>

                        </li>

                        <li class="sub-menu">
                            <a href="YorumListesi">
                                <i class="icon-cogs"></i>
                                <span>Yorumlar</span>
                            </a>
                            <ul class="sub">
                                <li><a href="/Admin/YorumListesi">Yorum Yönetimi</a></li>
                            </ul>
                        </li>
                        <li class="sub-menu">
                            <a href="/Content/Admin/javascript:;">
                                <i class="icon-book"></i>
                                <span>İlan Yönetimi</span>
                            </a>
                            <ul class="sub">
                                <li><a href="/Admin/ilanListele">İlan Listesi</a></li>
                                <li><a href="/Admin/konutEkle">İlan Ekle</a></li>
                            </ul>
                        </li>
                        <li class="sub-menu">
                            <a href="Home/Index">
                                <i class="icon-tasks"></i>
                                <span>Siteye Git</span>
                            </a>

                        </li>
                        <li class="sub-menu">
                            <a href="/Content/Admin/javascript:;">
                                <i class="icon-tasks"></i>
                                <span>İstatistikler</span>
                            </a>
                            <ul class="sub">
                                <li><a href="/Content/Admin/form_component.html">Form Components</a></li>
                            </ul>
                        </li>
                    </ul>
                    <!-- sidebar menu end-->
                </div>
                <!-- sidebar menu end-->
            </div>
        </aside>
        <!--sidebar end-->
        <!--main content start-->
        <section id="main-content">
            <section class="wrapper">
                <div class="row state-overview">
                    <div class="col-lg-3 col-sm-6">
                        <section class="panel">
                            <div class="symbol terques">
                                <i class="icon-user"></i>
                            </div>
                            <div class="value">
                                <h1 class="count">
                                    0
                                </h1>
                                <p>New Users</p>
                            </div>
                        </section>
                    </div>
                    <div class="col-lg-3 col-sm-6">
                        <section class="panel">
                            <div class="symbol red">
                                <i class="icon-tags"></i>
                            </div>
                            <div class="value">
                                <h1 class=" count2">
                                    0
                                </h1>
                                <p>Sales</p>
                            </div>
                        </section>
                    </div>
                    <div class="col-lg-3 col-sm-6">
                        <section class="panel">
                            <div class="symbol yellow">
                                <i class="icon-shopping-cart"></i>
                            </div>
                            <div class="value">
                                <h1 class=" count3">
                                    0
                                </h1>
                                <p>New Order</p>
                            </div>
                        </section>
                    </div>
                    <div class="col-lg-3 col-sm-6">
                        <section class="panel">
                            <div class="symbol blue">
                                <i class="icon-bar-chart"></i>
                            </div>
                            <div class="value">
                                <h1 class=" count4">
                                    0
                                </h1>
                                <p>Yorum Sayisi</p>
                            </div>
                        </section>
                    </div>
                </div>
                @RenderBody()

            </section>
        </section>
        <!--main content end-->
        <!--footer start-->
        <footer class="site-footer">
            <div class="text-center">
                2013 &copy; FlatLab by VectorLab.
                <a href="/Content/Admin/#" class="go-top">
                    <i class="icon-angle-up"></i>
                </a>
            </div>
        </footer>
        <!--footer end-->
    </section>

    <!-- js placed at the end of the document so the pages load faster -->
    <script src="/Content/Admin/js/jquery.js"></script>
    <script src="/Content/Admin/js/jquery-1.8.3.min.js"></script>
    <script src="/Content/Admin/js/bootstrap.min.js"></script>
    <script class="include" type="text/javascript" src="/Content/Admin/js/jquery.dcjqaccordion.2.7.js"></script>
    <script src="/Content/Admin/js/jquery.scrollTo.min.js"></script>
    <script src="/Content/Admin/js/jquery.nicescroll.js" type="text/javascript"></script>
    <script src="/Content/Admin/js/jquery.sparkline.js" type="text/javascript"></script>
    <script src="/Content/Admin/assets/jquery-easy-pie-chart/jquery.easy-pie-chart.js"></script>
    <script src="/Content/Admin/js/owl.carousel.js"></script>
    <script src="/Content/Admin/js/jquery.customSelect.min.js"></script>
    <script src="/Content/Admin/js/respond.min.js"></script>

    <script class="include" type="text/javascript" src="/Content/Admin/js/jquery.dcjqaccordion.2.7.js"></script>

    <!--common script for all pages-->
    <script src="/Content/Admin/js/common-scripts.js"></script>

    <!--script for this page-->
    <script src="/Content/Admin/js/sparkline-chart.js"></script>
    <script src="/Content/Admin/js/easy-pie-chart.js"></script>
    <script src="/Content/Admin/js/count.js"></script>

    <script>

        //owl carousel

        $(document).ready(function () {
            $("#owl-demo").owlCarousel({
                navigation: true,
                slideSpeed: 300,
                paginationSpeed: 400,
                singleItem: true,
                autoPlay: true

            });
        });

        //custom select box

        $(function () {
            $('select.styled').customSelect();
        });

    </script>
    @RenderSection("script", false)



    </body>

</html>

